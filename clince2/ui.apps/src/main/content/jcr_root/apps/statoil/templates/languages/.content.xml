<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          jcr:description="Generic template for Statoil Language Pages"
          jcr:primaryType="cq:Template"
          jcr:title="Statoil Language Page"
          allowedPaths="[/content(/.*)?]"
          ranking="{Long}90">
    <jcr:content
            cq:allowedTemplates="[/apps/statoil/templates/page]"
            cq:template="/apps/statoil/templates/front"
            cq:designPath="/etc/designs/statoil"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="statoil/components/page/languages">
        <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/parsys"/>
    </jcr:content>
</jcr:root>
