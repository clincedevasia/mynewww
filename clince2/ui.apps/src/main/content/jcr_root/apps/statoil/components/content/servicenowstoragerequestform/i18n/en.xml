        jcr:primaryType="sling:MessageEntry"
        sling:key="servicenowstoragerequestform.entrycapacityplaceholder"
        sling:message="Maximum 770 000 Nm³ per hour"/>
    <exitcapacity
        jcr:primaryType="sling:MessageEntry"
        sling:key="servicenowstoragerequestform.exitcapacity"
        sling:message="Exit capacity in m³/h"/>
