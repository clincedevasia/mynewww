        jcr:primaryType="sling:MessageEntry"
        sling:key="servicenowstoragerequestform.contactperson-error"
        sling:message="Please enter the name of a contact person"/>
    <email
        jcr:primaryType="sling:MessageEntry"
        sling:key="servicenowstoragerequestform.email"
        sling:message="E-mail address"/>
