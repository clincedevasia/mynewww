        jcr:primaryType="sling:MessageEntry"
        sling:key="servicenowstoragerequestform.volume"
        sling:message="Volume of working gas in m³"/>
    <volumeplaceholder
        jcr:primaryType="sling:MessageEntry"
        sling:key="servicenowstoragerequestform.volumeplaceholder"
        sling:message="Maximum 200 000 000 Nm³"/>
